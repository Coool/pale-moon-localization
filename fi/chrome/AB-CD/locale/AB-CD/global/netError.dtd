<!ENTITY retry.label "Yritä uudestaan">

<!-- Specific error messages -->

<!ENTITY connectionFailure.title "Yhteyden muodostus epäonnistui">
<!ENTITY connectionFailure.longDesc "<p>Vaikka sivu vaikuttaa paikkansapitävältä, selain ei kyennyt muodostamaan yhteyttä.</p><ul><li>Voiko sivu olla väliaikaisesti poissa käytöstä? Yritä myöhemmin uudestaan.</li><li>Etkö pysty selaamaan muita sivustoja? Tarkista tietokoneen verkkoyhteys.</li><li>Onko tietokoneessasi käytössä palomuuri tai välityspalvelin? Väärät asetukset voivat häiritä Internetin käyttöä.</li></ul>">

<!ENTITY deniedPortAccess.title "Portti suljettu turvallisuussyistä">
<!ENTITY deniedPortAccess.longDesc "<p>Pyydetty osoite ilmoitti portin (esim. <q>palemoon.org:25</q> merkitsee palemoon.orgin porttia 25) jota käytetään normaalisti <em>muuhun</em> kuin verkkoselailuun. Selain on estänyt pyynnön turvallisuussyistä.</p>">

<!ENTITY dnsNotFound.title "Osoitetta ei löydy">
<!ENTITY dnsNotFound.longDesc "<p>Selain ei löytänyt annetun osoitteen isäntäpalvelinta.</p><ul><li>Teitkö virheen kirjoittaessasi osoitetta? (esim.<q><strong>ww</strong>.mozilla.org</q>  <q><strong>www</strong>.mozilla.org</q>:n sijasta?)</li><li>Oletko varma, että tämä on olemassa oleva osoite? Sen rekisteröinti voi olla vanhentunut.</li><li>Etkö pysty selaamaan muita sivustoja? Tarkista verkkoyhteytesi ja DNS-palvelinasetuksesi.</li><li>Onko tietokoneesi tai lähiverkkosi suojattu palomuurilla tai välityspalvelimella? Väärät asetukset saattavat haitata Internet-yhteyttä.</li></ul>">

<!ENTITY fileNotFound.title "Tiedostoa ei löytynyt">
<!ENTITY fileNotFound.longDesc "<ul><li>Voiko kohde olla nimetty uudelleen, poistettu tai siirretty uuteen sijaintiin?</li><li>Onko osoitteessa kirjoitusvirheitä?</li><li>Onko sinulla riittävät käyttöoikeudet pyydettyä kohdetta varten?</li></ul>">

<!ENTITY generic.title "Pyyntöä ei voida suorittaa">
<!ENTITY generic.longDesc "<p>Tähän ongelmaan tai virheeseen ei ole saatavana lisätietoja</p>">

<!ENTITY malformedURI.title "Virheellinen osoite">
<!ENTITY malformedURI.longDesc "<p>Pyydetty osoite ei ole tunnistettavassa muodossa. Tarkista sijaintipalkki virheiden varalta ja yritä uudelleen.</p>">

<!ENTITY netInterrupt.title "Tiedonsiirto keskeytynyt">
<!ENTITY netInterrupt.longDesc "<p>Selain muodosti yhteyden onnistuneesti, mutta yhteys keskeytyi kesken tiedonsiirron. Ole hyvä ja yritä uudelleen.</p><ul><li>Etko pysty selaamaan muita sivustoja? Tarkista tietokoneesi verkkoyhteys.</li><li>Ongelmien jatkuessa ota yhteys verkkosi pääkäyttäjään tai Internet-palveluntarjoajaan.</li></ul>">

<!ENTITY notCached.title "Dokumentti on vanhentunut">
<!ENTITY notCached.longDesc "<p>Pyydettyä dokumenttia ei ole tallennettu selaimen välimuistiin.</p><ul><li>Turvallisuussyistä selain ei automaattisesti lataa uudelleen luottamuksellisia dokumentteja.</li><li>Paina Yritä uudestaan ladataksesi dokumentin sivustolta uudestaan.</li></ul>">

<!ENTITY netOffline.title "Yhteydettömässä tilassa">
<!ENTITY netOffline.longDesc2 "<p>Selain on yhteydettömässä tilassa, eikä voi ladata pyydettyä kohdetta.</p><ul><li>Onko tietokone kytkettynä aktiiviseen verkkoon?</li><li>Paina &quot;Yritä uudestaan siirtyäksesi yhteydelliseen tilaan ja ladataksesi sivun uudestaan.</li></ul>">

<!ENTITY contentEncodingError.title "Sisällön koodausvirhe">
<!ENTITY contentEncodingError.longDesc "<p>Lataamaasi sivua ei voida näyttää, koska se käyttää virheellistä tai ei-tuettua pakkausmenetelmää.</p><ul><li>Ole hyvä, ja ota yhteyttä verkkosivun omistajaan ilmoittaaksesi virheestä.</li></ul>">

<!ENTITY unsafeContentType.title "Ei turvallinen tiedostotyyppi">
<!ENTITY unsafeContentType.longDesc "
<ul>
  <li>Ilmoita sivuston omistajalle tästä ongelmasta.</li>
</ul>
">

<!ENTITY netReset.title "Yhteys keskeytynyt">
<!ENTITY netReset.longDesc "<p>Verkon linkki katkesi yhteyttä muodostettaessa. Yritä uudestaan.</p>">

<!ENTITY netTimeout.title "Verkon aikakatkaisu">
<!ENTITY netTimeout.longDesc "<p>Palvelin ei vastannut yhteyspyyntöön, ja selain lopetti vastauksen odottamisen.</p><ul><li>Palvelin voi olla kovan rasituksen alainen tai väliaikaisesti huollettavana. Yritä myöhemmin uudestaan.</li><li>Jos muutkaan sivustot eivät toimi, tarkista tietokoneen verkkoasetukset.</li><li>Onko tietokone tai verkko suojattu palomuurilla tai käytetäänkö välityspalvelinta?  Virheelliset asetukset voivat haitata selaamista.</li><li>Jos ongelmat jatkuvat, ota yhteyttä verkon ylläpitoon tai verkkoyhteyden palveluntarjoajaan.</li></ul>">

<!ENTITY unknownProtocolFound.title "Tuntematon protokolla">
<!ENTITY unknownProtocolFound.longDesc "<p>Osoite määrittää protokollan (esimerkiksi <q>wxyz: / /</q>), jota selain ei tunnista, joten selain ei saa kunnolla yhteyttä sivustoon.</p> <ul><li>Yrittätkö käyttää multimedia- tai muita tekstittömiä palveluja? Tarkistaa sivuston eritysvaatimukset.</li> <li>Jotkin protokollat voivat vaatia kolmannen osapuolen ohjelmistoa tai liitännäistä ennen kuin selain voi tunnistaa ne.</li></ul>">

<!ENTITY proxyConnectFailure.title "Välityspalvelin kieltäytyi yhteydestä">
<!ENTITY proxyConnectFailure.longDesc "<p>Selain on asetettu käyttämään välityspalvelinta, mutta välityspalvelin ei hyväksynyt yhteyttä.</p><ul><li>Ovatko selaimen välityspalvelinasetukset oikeat? Tarkista asetukset ja yritä uudelleen.</li><li>Tulisiko välityspalvelimen hyväksyä yhteydet tästä verkkoyhteydestä?</li><li>Jos ongelmat jatkuvat, ota yhteyttä verkon ylläpitoon tai verkkoyhteyden palveluntarjoajaan.</li></ul>">

<!ENTITY proxyResolveFailure.title "Välityspalvelinta ei löydy">
<!ENTITY proxyResolveFailure.longDesc "<p>Selain on asetettu käyttämään välityspalvelinta, mutta siihen ei saatu yhteyttä.</p><ul><li>Ovatko selaimen välityspalvelinasetukset oikeat? Tarkista asetukset ja yritä uudelleen.</li><li>Onko tietokoneen verkkoyhteys toimintakykyinen?</li><li>Jos ongelmat jatkuvat, ota yhteyttä verkon ylläpitoon tai verkkoyhteyden palveluntarjoajaan.</li></ul>">

<!ENTITY redirectLoop.title "Uudelleenohjaussilmukka">
<!ENTITY redirectLoop.longDesc "<p>Selain on lopettanut pyydetyn kohteen lataamisen. Palvelin uudelleenohjaa yhteyspyyntöjä loputtomasti.</p><ul><li>Onko kaikki tai sivuston tarvitsemat evästeet estetty?</li><li><em>HUOM</em>: Jos sivuston evästeiden salliminen ei korjaa ongelmaa, vika on luultavasti palvelimen asetuksissa, eikä tässä tietokoneessa.</li></ul>">

<!ENTITY unknownSocketType.title "Väärä vastaus">
<!ENTITY unknownSocketType.longDesc "<p>Sivusto vastasi verkkopyyntöön odottamattomalla tavalla ja selain joutui keskeyttämään toiminnon.</p>">

<!ENTITY nssFailure2.title "Suojatun yhteyden muodostaminen epäonnistui">
<!ENTITY nssFailure2.longDesc2 "<p>Avattavaa sivua ei voida näyttää, koska vastaanotetun datan alkuperää ei kyetty varmentamaan.</p><ul><li>Ilmoitathan ongelmasta sivuston omistajalle.</li></ul>">

<!ENTITY nssBadCert.title "Suojatun yhteyden muodostaminen epäonnistui">
<!ENTITY nssBadCert.longDesc2 "<ul>
 <li>Ongelma voi johtua palvelimen asetuksista tai jonkin toisen palvelimen 
 vilpillisestä yrityksestä tekeytyä palvelimeksi.</li>
 <li>Jos yhteyden muodostuminen palvelimeen on aiemmin onnistunut, vika voi
 olla väliaikainen. Yritä tällöin myöhemmin uudestaan.</li>
</ul>
">

<!ENTITY malwareBlocked.title "Epäilty hyökkäyssivusto!">
<!ENTITY malwareBlocked.longDesc "
<p>Hyökkäyssivustot yrittävät asentaa ohjelmia, jotka varastavat henkilökohtaisia tietoja, käyttävät tietokonettasi ulkopuolisiin hyökkäyksiin tai vaurioittavat järjestelmääsi.</p>
<p>Sivuston omistaja, joka uskoo sivustonsa raportoidun virheellisesti hyökkäyssivustoksi, voi <a href='https://www.stopbadware.org/request-review' > pyytää tarkistusta</a>.</p>
">

<!ENTITY phishingBlocked.title "Epäilty huijaussivusto!">
<!ENTITY phishingBlocked.longDesc "
<p>Henkilötietojen kirjoittaminen tälle sivulle voi johtaa identiteettivarkauteen tai johonkin muuhun petokseen.</p>
<p>Tämän tyyppiset verkkohuijaukset ovat osa phishing- tai kalastushuijauksia, joissa sivuilla tai sähköposteilla yritetään jäljitellä mahdollisesti luottamiesi toimijoiden viestintää.</p>
">

<!ENTITY cspBlocked.title "Estetty sisällön suojauskäytännön perusteella">
<!ENTITY cspBlocked.longDesc "<p>Selain esti tämän sivun latautumisen tässä yhteydessä, koska sivun sisällön tietosuojakäytäntö kieltää sen.</p>">

<!ENTITY corruptedContentError.title "Sisältö vioittunut virhe">
<!ENTITY corruptedContentError.longDesc "<p>Avattavaa sivua ei voida näyttää, koska tiedonsiirrossa tapahtui virhe.</p><ul><li>Ilmoitathan ongelmasta sivuston omistajalle.</li></ul>">

<!ENTITY remoteXUL.title "XUL-koodia etänä">
<!ENTITY remoteXUL.longDesc "<p><ul><li>Ilmoitathan ongelmasta sivuston omistajalle.</li></ul></p>">

